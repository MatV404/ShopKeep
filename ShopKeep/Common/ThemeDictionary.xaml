<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Light">
            <SolidColorBrush x:Key="BackgroundColor">#F5E6E8</SolidColorBrush>
            <SolidColorBrush x:Key="InteractiveElementColor">#DCCFD1</SolidColorBrush>
            <SolidColorBrush x:Key="OverlayBarColor">#967AA1</SolidColorBrush>
            <SolidColorBrush x:Key="RegularTextColor">#584E78</SolidColorBrush>
            <SolidColorBrush x:Key="HeadingColor">#192A51</SolidColorBrush>
        </ResourceDictionary>
        <ResourceDictionary x:Key="Dark">
            <SolidColorBrush x:Key="BackgroundColor">#F5E6E8</SolidColorBrush>
            <SolidColorBrush x:Key="InteractiveElementColor">#DCCFD1</SolidColorBrush>
            <SolidColorBrush x:Key="OverlayBarColor">#967AA1</SolidColorBrush>
            <SolidColorBrush x:Key="RegularTextColor">#584E78</SolidColorBrush>
            <SolidColorBrush x:Key="HeadingColor">#192A51</SolidColorBrush>
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>
